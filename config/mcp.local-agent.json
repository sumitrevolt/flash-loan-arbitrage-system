{
    "mcpServers": {
        "github-copilot": {
            "command": "npx",
            "args": [
                "@modelcontextprotocol/server-github",
                "--github-token",
                "${GITHUB_TOKEN}"
            ],
            "env": {
                "GITHUB_TOKEN": "your_github_token_here"
            }
        },
        "local-agent": {
            "command": "python",
            "args": [
                "path/to/your/local_agent.py"
            ],
            "env": {
                "MY_ENV_VAR": "value"
            }
        }
    }
}
